{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u--(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue"
    }
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "AiTa-Hv"
      }
    },
    {
      "path": "pages/CatFace/index",
      "style": {
        "navigationBarTitleText": "CatFace"
      }
    },
    {
      "path": "pages/CatFace/database",
      "style": {
        "navigationBarTitleText": "CatFace-DB"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "AiTa-CatFace",
    "navigationBarBackgroundColor": "#e8f3f1",
    "backgroundColor": "#F8F8F8"
  }
}
